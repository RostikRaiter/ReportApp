@model IEnumerable<UniversityReportApp.Domain.Entities.Department>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Faculty Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.FacultyId</td>
            </tr>
        }
    </tbody>
</table>
