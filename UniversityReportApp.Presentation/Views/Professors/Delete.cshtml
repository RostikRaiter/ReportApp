@model UniversityReportApp.Domain.Entities.Professor

<h2>Delete Professor</h2>

<form asp-action="Delete">
    <div class="form-group">
        <label asp-for="Id" class="control-label"></label>
        <input asp-for="Id" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="FirstName" class="control-label"></label>
        <input asp-for="FirstName" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="MiddleName" class="control-label"></label>
        <input asp-for="MiddleName" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="LastName" class="control-label"></label>
        <input asp-for="LastName" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="Email" class="control-label"></label>
        <input asp-for="Email" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="DepartmentId" class="control-label"></label>
        <input asp-for="DepartmentId" class="form-control" readonly />
    </div>
    <div class="form-group">
        <input type="submit" value="Confirm Delete" class="btn btn-danger" />
    </div>
</form>
